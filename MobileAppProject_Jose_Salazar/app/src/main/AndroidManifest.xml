<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.josesalazar.project_jose_salazar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ShapeSquare"></activity>
        <activity android:name=".ShapeCircle" />
        <activity android:name=".ActivityShapes" />
        <activity android:name=".SettingsFragment" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".ColorPurple" />
        <activity android:name=".ColorOrange" />
        <activity android:name=".ColorYellow" />
        <activity android:name=".ColorGreen" />
        <activity android:name=".ColorBlue" />
        <activity android:name=".ColorRed" />
        <activity android:name=".ColorsPage" />
        <activity android:name=".ActivityColors" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>